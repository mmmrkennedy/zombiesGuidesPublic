{
    "tabWidth": 4,
    "singleQuote": false,
    "semi": true,
    "trailingComma": "all",
    "printWidth": 120,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "crlf",
    "overrides": [
        {
            "files": "*.html",
            "options": { "printWidth": 200 }
        }
    ]
}
